# Config driver menu
menu "Generic Device Driver"

config SERIAL
	tristate "Enable serial support"
	default n

config BOCHSVBE
	select HAL_PCI_CONTROLLER
	tristate "Enable Bochs VBE support"
	default n

config DISK
	tristate "Enable LBA disk support"
	default n

config PS2KEYBOARD
	tristate "Enable ps2 key board input support"
	default n

config PS2MOUSE
	select HAL_PS2_CONTROLLER
	tristate "Enable ps2 mouse input support"
	default n
	
endmenu
