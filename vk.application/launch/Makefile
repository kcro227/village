######################################
# applications
######################################
apps-$(CONFIG_LAUNCH)                += launch


######################################
# paths
######################################
inc-launch-$(CONFIG_LAUNCH)          += vk.application/launch/inc

src-launch-$(CONFIG_LAUNCH)          += vk.application/launch/src


######################################
# objects
######################################
objs-launch-$(CONFIG_LAUNCH)         += Launch.o


######################################
# libraries
######################################
libs-launch-$(CONFIG_LAUNCH)         += kernel graphics
