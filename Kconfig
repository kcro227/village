comment "This is using to configure GUI"

# config GUI

config GUI
	bool "Enable GUI support"
	default n

# config wedget

menu "Wedget"
	depends on GUI 

config MENU
	bool "Enable menu support"
	default y

config BUTTON
	bool "Enable button support"
	default y

config LISTBOX
	bool "Enable list box support"
	default y

endmenu
